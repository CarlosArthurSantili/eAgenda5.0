<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<add
		  connectionString="Data Source=(localdb)\mssqllocaldb;Initial Catalog=eAgenda.Database;Integrated Security=True;Pooling=False"
		  name="dbsqlserver"
		  providerName="System.Data.SqlClient"
    />
		<add
		  connectionString="data source=..\..\..\eAgenda.Controladores\Database\DBeAgenda.db"
		  name="dbsqlite"
		  providerName="System.Data.SQLite"
    />
	</connectionStrings>
	<appSettings>
		<add key ="bancodedados" value="dbsqlserver"/>
	</appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>